type Request {
	Name string `path:"name,options=you|me"`
}

type Response {
	Message string `json:"message"`
}
type RegistereReq {
	Name string `form:"name"`
	Pwd string `form:"pwd"`
}
type RegisterResp {
	Ok bool `json:"ok"`
}

@server(
	group : user
)
service gateway-api {
	@handler GatewayHandler
	get /from/:name (Request) returns (Response);
	@handler RegisterHandler
	get /register (RegistereReq) returns (RegisterResp)
	@handler testHandler
	get /test
}